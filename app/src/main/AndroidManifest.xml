<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gamechanger.easy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Delete_Book"></activity>
        <activity android:name=".user.Submit_book" />
        <activity android:name=".user.Issue_book" />
        <activity android:name=".User_dashboard" />
        <activity android:name=".Show_book_Activity" />
        <activity android:name=".Add_Book_Activity" />
        <activity android:name=".Admin_Dashboard_Activity" />
        <activity android:name=".AdminLoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>